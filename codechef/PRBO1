#include <iostream>
#include<set>
#include<vector>
#define lli long long int
#include<algorithm>
#include<cmath>
using namespace std;

int main() {
	// your code goes here
	lli n,m,a,k,t,f;
	
	// c=0;
	cin>>t;
	while(t--){
		f=0;
		cin>>n;
		
		
		for(lli i=2;i<sqrt(n);i++){
			
			if(n%i==0){
				f=1;
				break;
			}
		
		}
		if(f==1)cout<<"no"<<endl;
		else
			cout<<"yes"<<endl;
	}
	// cout<<a;
	return 0;
}
